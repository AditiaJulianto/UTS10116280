<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical">

    <ImageView
        android:id="@+id/profil"
        android:layout_width="match_parent"
        android:layout_height="654dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="77dp"
        android:src="@drawable/prrofile" />

    <TextView
        android:id="@+id/ig"
        android:layout_width="165dp"
        android:layout_height="58dp"
        android:layout_alignTop="@+id/profil"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="175dp"
        android:layout_marginTop="351dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="322dp"
        android:clickable="true"
        android:contentDescription="Instagram"
        android:text="@julianaditia"
        android:textColor="#DF26FF"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/fb"
        android:layout_width="209dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/profil"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="168dp"
        android:layout_marginTop="459dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="221dp"
        android:onClick="OpenFacebook"
        android:contentDescription="Facebook"
        android:text="@Aditia Julianto"
        android:textColor="#3F51B5"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/wa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="179dp"
        android:layout_marginTop="571dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="128dp"
        android:onClick="082249031347"
        android:text="082249031347"
        android:textColor="#2A882E"
        android:textSize="24sp" />
</RelativeLayout>